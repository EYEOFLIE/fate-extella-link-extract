// <"type":"attack"＞
//	MAXは12コマンド、必要なら追加可能
//	DA:ダッシュ　JU:ジャンプ　LA:ライトニングアーツ　SP:宝具
//	上記以外は全てモーションコマンド名として認識
//
// <"type":"evade">
//  far_away:目標から離れる
//  jump_over:目標を飛び越える
//  consecutive_jump:目標の周りで連続ジャンプして相手をかく乱する
//
// <"type":"wait">
//	その場で立ち止まって待機
//
{
	// HP50%以上ならこのモード
	"normal":
	[
		// ----	攻撃 ----
		{
			"type":	"attack",		// 攻撃
			"command":[			// モーションコマンド
				"ATTACK_W1",			// 弱1
				"ATTACK_W2",			// 弱2
				"ATTACK_W7",			// 弱7
				"ATTACK_W8"			// 弱8
			],
			"percent":100,			// 発生確率（合計値を100%として換算）
			"range":4,			// 発動距離（メートル）
			"angle":60			// 発動角度
		},
		{
			"type":"attack",			// 攻撃
			"command":[			// モーションコマンド
				"ATTACK_S1",			// 強1-1
				"ATTACK_S2",			// 強1-2
				"ATTACK_S3"			// 強1-3
			],
			"percent":100,			// 発生確率（合計値を100%として換算）
			"range":5,			// 発動距離（メートル）
			"angle":60			// 発動角度
		},
		{
			"type":	"attack",		// 攻撃
			"command":[			// モーションコマンド
				"ATTACK_C1",			// 強2
				"JUMP_ATTACK_S1"		// ジャンプ強1
			],
			"percent":100,			// 発生確率（合計値を100%として換算）
			"range":5,			// 発動距離（メートル）
			"angle":60			// 発動角度
		},

		// ----	回避 ----
		{
			"type":"evade",			// 回避
			"command":"far_away",		// 「目標から離れる」
			"percent":100,			// 発生確率（合計値を100%として換算）
			"start_dis_min":-1,		// 最小発動距離（メートル）0＞で無制限
			"start_dis_max":5,		// 最大発動距離（メートル）0＞で無制限
			"temp_float":[
				120.0,			// 回避方向の最小角度（目標対自身）
				240.0,			// 回避方向の最大角度（目標対自身）
				11.0,			// 最小回避距離
				12.0,			// 最大回避距離
				1			// ダッシュ許可権(＞0 ダッシュ可能)
			]
		}
	]

